{
  "name": "@stnew/ratatouille",
  "version": "0.0.3",
  "description": "stnew react component library",
  "scripts": {
    "storybook": "NODE_ENV=development start-storybook -s ./shared -p 9001",
    "prestart": "npm install && npm run build",
    "start": "npm-run-all --parallel storybook build:watch",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint packages/**/src/**/*",
    "bootstrap": "lerna bootstrap --no-ci",
    "build": "lerna exec --parallel npm run build --ignore @stnew/eslint*",
    "export-static-storybook": "build-storybook -c .storybook -o .out",
    "publish-packages": "lerna publish",
    "prepublish-packages": "npm run build",
    "postinstall": "npm run bootstrap",
    "new-pkg": "hygen package new --",
    "new-comp": "hygen package new --component --",
    "new-styled-comp": "hygen package new --component --",
    "new-fc": "hygen package new --component --functional --",
    "new-styled-fc": "hygen package new --component --functional --styled --"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:somethingnew-co/ratatouille.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/somethingnew-co/ratatouille/issues"
  },
  "devDependencies": {
    "@stnew/eslint-config": "*",
    "@stnew/eslint-config-react": "*",
    "@stnew/eslint-config-typescript": "*",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-viewport": "^6.3.12",
    "@storybook/addons": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@styled-system/css": "^5.1.5",
    "@styled-system/should-forward-prop": "^5.1.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/enzyme": "^3.10.10",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/storybook__addon-actions": "^5.2.1",
    "@types/storybook__addon-info": "^5.2.4",
    "@types/storybook__addon-knobs": "^5.2.1",
    "@types/storybook__addon-links": "^5.2.1",
    "@types/storybook__addon-viewport": "^5.2.1",
    "@types/storybook__react": "^5.2.1",
    "@types/styled-system__css": "^5.0.16",
    "@types/styled-components": "^5.1.15",
    "@types/styled-system__should-forward-prop": "^5.1.2",
    "@types/styled-system__theme-get": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "all-contributors-cli": "^6.20.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.2.3",
    "enzyme": "^3.11.0",
    "eslint-plugin-unicorn": "^37.0.1",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "glob-loader": "^0.3.0",
    "husky": "^7.0.4",
    "hygen": "^6.1.0",
    "jest": "^27.3.1",
    "jest-styled-components": "^7.0.5",
    "lerna": "^4.0.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^17.0.2",
    "storybook-readme": "^5.0.9",
    "styled-components": "^5.3.3",
    "styled-map": "^3.3.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  }
}
