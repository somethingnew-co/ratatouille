{
  "name": "@stnew/ratatouille",
  "version": "0.0.3",
  "description": "stnew react component library",
  "scripts": {
    "storybook": "NODE_ENV=development start-storybook -s ./shared -p 9001",
    "prestart": "npm install && npm run build",
    "start": "npm-run-all --parallel storybook build:watch",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint packages/**/src/**/*",
    "bootstrap": "lerna bootstrap --no-ci",
    "build": "lerna exec --parallel npm run build --ignore @stnew/eslint*",
    "export-static-storybook": "build-storybook -c .storybook -o .out",
    "publish-packages": "lerna publish",
    "prepublish-packages": "npm run build",
    "postinstall": "npm run bootstrap",
    "new-pkg": "hygen package new --",
    "new-comp": "hygen package new --component --",
    "new-styled-comp": "hygen package new --component --",
    "new-fc": "hygen package new --component --functional --",
    "new-styled-fc": "hygen package new --component --functional --styled --"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:somethingnew-co/ratatouille.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/somethingnew-co/ratatouille/issues"
  },
  "devDependencies": {
    "@stnew/eslint-config": "*",
    "@stnew/eslint-config-typescript": "*",
    "@storybook/addon-a11y": "^5.3.18",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@testing-library/react": "^10.0.2",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/storybook__addon-actions": "^5.2.1",
    "@types/storybook__addon-info": "^5.2.1",
    "@types/storybook__addon-knobs": "^5.2.1",
    "@types/storybook__addon-links": "^5.2.1",
    "@types/storybook__addon-viewport": "^5.2.1",
    "@types/storybook__react": "^5.2.1",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "all-contributors-cli": "^6.14.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "glob-loader": "^0.3.0",
    "husky": "^4.2.5",
    "hygen": "^5.0.3",
    "jest": "^25.3.0",
    "jest-dom": "^4.0.0",
    "jest-styled-components": "^7.0.2",
    "lerna": "^3.20.2",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "storybook-readme": "^5.0.8",
    "styled-components": "^5.1.0",
    "styled-map": "^3.3.0",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  }
}
