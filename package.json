{
  "name": "@stnew/ratatouille",
  "version": "0.0.1",
  "description": "stnew react component library",
  "scripts": {
    "storybook": "NODE_ENV=development start-storybook -p 9001",
    "prestart": "npm install && npm run build",
    "start": "npm-run-all --parallel storybook build:watch",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint packages/**/src/**/*",
    "bootstrap": "lerna bootstrap",
    "build": "lerna exec --parallel -- babel src -d dist --ignore **/*.spec.js,**/*.stories.js",
    "build:watch": "npm run build -- --watch",
    "export-static-storybook": "build-storybook -c .storybook -o .out",
    "publish-packages": "lerna publish",
    "prepublish-packages": "npm run build",
    "postinstall": "npm run bootstrap"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:somethingnew-co/ratatouille.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/somethingnew-co/ratatouille/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-knobs": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "@types/jest": "^24.0.17",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "all-contributors-cli": "^6.8.1",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "glob-loader": "^0.3.0",
    "husky": "^3.0.3",
    "jest": "^24.8.0",
    "jest-dom": "^4.0.0",
    "lerna": "^3.16.4",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "styled-components": "^4.3.2",
    "styled-map": "^3.3.0",
    "typescript": "^3.5.3"
  }
}
