{
  "name": "@stnew/ratatouille",
  "version": "0.0.3",
  "description": "stnew react component library",
  "scripts": {
    "storybook": "NODE_ENV=development start-storybook -s ./shared -p 9001",
    "prestart": "npm install && npm run build",
    "start": "npm-run-all --parallel storybook build:watch",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint packages/**/src/**/*",
    "bootstrap": "lerna bootstrap --no-ci",
    "build": "lerna exec --parallel npm run build --ignore @stnew/eslint*",
    "export-static-storybook": "build-storybook -c .storybook -o .out",
    "publish-packages": "lerna publish",
    "prepublish-packages": "npm run build",
    "postinstall": "npm run bootstrap",
    "new-pkg": "hygen package new"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:somethingnew-co/ratatouille.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/somethingnew-co/ratatouille/issues"
  },
  "devDependencies": {
    "@stnew/eslint-config": "*",
    "@stnew/eslint-config-typescript": "*",
    "@storybook/addon-a11y": "^5.1.11",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-info": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addon-viewport": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.17",
    "@types/lodash": "^4.14.137",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__addon-info": "^4.1.2",
    "@types/storybook__addon-knobs": "^5.0.3",
    "@types/storybook__addon-links": "^3.3.5",
    "@types/storybook__addon-viewport": "^4.1.1",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "all-contributors-cli": "^6.8.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "glob-loader": "^0.3.0",
    "husky": "^3.0.3",
    "hygen": "^4.0.8",
    "jest": "^24.8.0",
    "jest-dom": "^4.0.0",
    "lerna": "^3.16.4",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-docgen-typescript-loader": "^3.1.1",
    "react-dom": "^16.9.0",
    "storybook-readme": "^5.0.8",
    "styled-components": "^4.3.2",
    "styled-map": "^3.3.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  }
}
